journal = {doi.org},
month = {sep},
pages = {190124},
publisher = {Cold Spring Harbor Laboratory},
title = {{Accurate Genomic Prediction Of Human Height}},
url = {https://www.biorxiv.org/content/early/2017/09/19/190124},
year = {2017}
}
