issn = {2045-8827},
journal = {MicrobiologyOpen},
keywords = {Cellulose anaerobic digestion,FISH,SIMSISH,SIP,WWE1 candidate division},
month = {apr},
number = {2},
pages = {157--67},
pmid = {24497501},
publisher = {Wiley-Blackwell},
title = {{Members of the uncultured bacterial candidate division WWE1 are implicated in anaerobic digestion of cellulose.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24497501 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3996565},
